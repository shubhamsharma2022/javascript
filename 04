console.log("Hello, World!");
let i = 1;
let j = 0;
let k = 1;
let string = "" ;

for (i = 1 ; i <= 5 ; i++) {

     for (j = 0 ; j <= i ; j++) {

         string += " ";
     }
     
     for (k = 5; k >= i ; k--) {
         
         string += "*"
     }
    string += "\n";
}
console.log(string);
